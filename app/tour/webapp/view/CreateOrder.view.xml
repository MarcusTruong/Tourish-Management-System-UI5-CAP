<mvc:View
  controllerName="tourish.management.controller.CreateOrder"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns="sap.m"
  xmlns:layout="sap.ui.layout">
  <Page id="_IDGenPage3" title="{i18n>createOrderTitle}" showNavButton="true" navButtonPress="onNavBack">
    <content>
      <layout:VerticalLayout id="_IDGenVerticalLayout1">
        <Input id="customerID" value="uuid-customer-1" placeholder="Customer ID"/>
        <Input id="tourID" value="uuid-tour-1" placeholder="Tour ID"/>
        <Button id="_IDGenButton4" text="Create" press="onCreate"/>
      </layout:VerticalLayout>
    </content>
  </Page>
</mvc:View>