<!-- File: tourishui/fragment/TableSettings.fragment.xml -->
<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    
    <Dialog id="_IDGenDialog"
        title="Table Settings"
        draggable="true"
        resizable="true">
        
        <VBox id="_IDGenVBox8" class="sapUiSmallMargin">
            <Label id="_IDGenLabel38" text="Visible Columns" class="sapUiSmallMarginBottom" />
            <List id="_IDGenList"
                mode="MultiSelect"
                includeItemInSelection="true"
                items="{/items}">
                <StandardListItem id="_IDGenStandardListItem1" title="{text}" />
            </List>
        </VBox>
        
        <buttons>
            <Button id="_IDGenButton19" text="Cancel" press=".onTableSettingsCancel" />
            <Button id="_IDGenButton25" text="OK" press=".onTableSettingsConfirm" type="Emphasized" />
        </buttons>
        
    </Dialog>
    
</core:FragmentDefinition>