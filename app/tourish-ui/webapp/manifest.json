{
  "_version": "1.65.0",
  "sap.app": {
    "id": "tourishui",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.17.3",
      "toolsId": "38399362-21b9-41b4-b6f0-2cfe93920cb7"
    },
    "dataSources": {
      "mainService": {
        "uri": "/user-service/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.134.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.tnt": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "tourishui.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "side": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/sideContent.json"
      },
      "auth": {
        "type": "sap.ui.model.json.JSONModel"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "tourishui.view",
        "async": true
      },
      "routes": [
        {
          "name": "login",
          "pattern": "login",
          "target": "login"
        },
        {
          "name": "register",
          "pattern": "register",
          "target": "register"
        },
        {
          "name": "dashboard",
          "pattern": "dashboard",
          "target": "dashboard"
        },
        {
          "name": "manageUsers",
          "pattern": "manageUsers",
          "target": "manageUsers"
        },
        {
          "name": "changePassword",
          "pattern": "changePassword",
          "target": "changePassword"
        },
        {
          "name": "myWorkspace",
          "pattern": "myWorkspace",
          "target": "myWorkspace"
        },
        {
          "name": "myProfile",
          "pattern": "myProfile",
          "target": "myProfile"
        }
      ],
      "targets": {
        "login": {
          "viewId": "Login",
          "viewName": "Login",
          "controlAggregation": "pages",
          "controlId": "app"
        },
        "register": {
          "viewId": "Register",
          "viewName": "Register",
          "controlAggregation": "pages",
          "controlId": "app"
        },
        "appContent": {
          "viewId": "AppContent", 
          "viewName": "AppContent",
          "controlAggregation": "pages",
          "controlId": "app"
        },
        "dashboard": {
          "viewId": "Dashboard",
          "viewName": "Dashboard",
          "controlId": "mainContents",
          "controlAggregation": "pages",
          "parent": "appContent"
        },
        "manageUsers": {
          "viewId": "ManageUsers",
          "viewName": "ManageUsers"
        },
        "myWorkspace": {
          "viewId": "MyWorkspace",
          "viewName": "MyWorkspace",
          "controlId": "mainContents",
          "controlAggregation": "pages",
          "parent": "appContent"
        },
        "myProfile": {
          "viewId": "MyProfile",
          "viewName": "MyProfile",
          "controlId": "mainContents",
          "controlAggregation": "pages",
          "parent": "appContent"
        },
        "notFound": {
          "viewId": "NotFound",
          "viewName": "NotFound"
        }
      }
    },
    "rootView": {
      "viewName": "tourishui.view.App",
      "type": "XML",
      "id": "app",
      "async": true
    }
  }
}